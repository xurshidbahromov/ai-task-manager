{
    "functions": {
        "server/index.py": {
            "runtime": "@vercel/python"
        }
    },
    "rewrites": [
        {
            "source": "/api",
            "destination": "server/index.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "server/index.py"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/frontend/dist/assets/$1"
        },
        {
            "source": "/((?!api|assets).*)",
            "destination": "/frontend/dist/index.html"
        }
    ]
}